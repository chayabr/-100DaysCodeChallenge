package com.vc;

public class MoveZeros {
	    public void moveZeroes(int[] nums) {
	        int slow=-1;
	        for(int fast=0;fast<nums.length;fast++){
	            if(nums[fast]!=0){
	                slow++;
	                int temp=nums[slow];
	                nums[slow]=nums[fast];
	                nums[fast]=temp;
	            }
	        }
	    }
	}


